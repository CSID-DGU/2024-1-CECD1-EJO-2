#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules/vite/bin/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules/vite/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules/vite/bin/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules/vite/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/vite@5.0.12_@types+node@20.8.10_sass@1.72.0/node_modules:/Users/simseohyeon/2024/react/2024-1CECD1-EJO-2-Frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
